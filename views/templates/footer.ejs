<script>
    $(document).ready(function(){
        $('#toggleButton').click(function(){
          // $('.alert').show()
          if(confirm('Do you want to continue?')){
            // alert("This is an alert message!"); 
              // $('#status').toggle();
              var status = 'N'
              if ($('#toggleButton').is(':checked')) {
                  $('#status').text('Verified');
                  status = 'Y'
              } else {
                  $('#status').text('Not Verified');
                  status = 'N'
              }
              
              $.ajax({
                    url:'/user/update_profile_verify',
                    method: 'POST',
                    data: {flag:status, id: $('#userTableId').val(), phone: $('#phone_no').val(), pro_id: $('#pro_id').val()},
                    success: function(result){
                      console.log(result);
                    },
                    error: function(err){
                      console.log(err);
                    }
                  })
                }else{
                  return false;
                }
              });
      });
</script>
<script>
  $(document).ready(function(){
      $('#ac_active_flag').click(function(){
          // $('#status').toggle();
          if(confirm('Do you want to continue?')){

            var ac_status = 'N'
            if ($('#ac_active_flag').is(':checked')) {
                $('#ac_status').text('Active');
                ac_status = 'Y'
            } else {
                $('#ac_status').text('Deactive');
                ac_status = 'N'
            }
            
            $.ajax({
                url:'/user/update_active_flag',
                method: 'POST',
                  data: {flag:ac_status, id: $('#userTableId').val(), phone: $('#phone_no').val(), pro_id: $('#pro_id').val()},
                  success: function(result){
                    console.log(result);
                  },
                  error: function(err){
                    console.log(err);
                  }
                })
          }else{
            return false;
          }
      });
  });
</script>
